LECTURE 2, EXAMPLE 1

class Band {
	public $vocals;
	public $drummer;
	public $guitar;
	public $bass;
	        function __construct($title, $vocals, $drummer, $guitar, $bass) {
	        $this->leadVocals = $vocals;
	        $this->drummer = $drummer;
	        $this->guitar = $guitar;
	        $this->bass = $bass;
	        }

            function get leadVocals() {
            return "{$this->leadVocals}" .
            "{$this->lastName}";
            }
}

 class Song {
   public $openingRiff;
   public $chorus;
   public $solo;
            function __construct($openingRiff, $chorus, $solo) {
            $this->openingRiff = $openingRiff;
            $this->chorus = $chorus;
            $this->solo = $solo;
            }

            function get openingRiff(){
            return "{$this->openingRiff}" .
            }
 }

  class Guitar {
    public $strings;
    public $frets;
    piblic $body;
    public $neck;
    public $headstock;
            function __construct($strings, $frets, $body, $neck, $headstock) {
            $this->strings = $strings;
            $this->frets = $frets;
            $this->body = $body;
            $this->neck = $neck;
            $this->headstock = $headstock
            }

            function get strings(){
            return "{$this->strings}" .
            }
  }

